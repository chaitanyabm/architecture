<h1> Enter Your new Project</h1>
<%= form_for @builder do |f| %>
<div class="form-group">
    
    <label> Enter your Builder name</label>
    <%= f.text_field :bname,class: "form-control" ,:required => true %><br>
</div>
<div class="form-group">
    
    <label> Enter your Location</label>
    <%= f.text_field :location,class: "form-control" ,:required => true %><br>
</div>
<div class="form-group">
    <label> Enter your project name</label>
    <%= f.text_field :project,class: "form-control" ,:required => true %> <br>
</div>
<div class="form-group">
    <label> Enter your bhk details</label>
    <%= f.text_field :bhk_details , class: "form-control", :required=> true%><br>
</div>
    <%= f.submit "Submit", class: "btn btn-primary"%>
</div>
<% end %>
